<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <!--provider setting-->
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">
                tieuminh_sumup1_post_listing.tieuminh_sumup1_post_listing_data_source
            </item>
            <item name="deps" xsi:type="string">tieuminh_sumup1_post_listing.tieuminh_sumup1_post_listing_data_source
            </item>
        </item>
        <item name="spinner" xsi:type="string">spinner_columns</item>
        <item name="buttons" xsi:type="array">
            <item name="add" xsi:type="array">
                <item name="name" xsi:type="string">add</item>
                <item name="label" xsi:type="string" translate="true">Add New Post</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">sumup1/post/creating</item>
            </item>
        </item>
    </argument>
    <!--provider setting-->
    <!--toolbar setting-->
    <listingToolbar name="listing_top">
        <settings>
            <sticky>true</sticky>
        </settings>
<!--        <bookmark name="bookmarks"/>-->
<!--        <columnsControls name="columns_controls"/>-->
<!--           <filterSearch name="fulltext"/>-->
        <!--filter setting-->
        <filters name="listing_filters">
         <!--   <settings>
                <templates>
                    <filters>
                        <select>
                            <param name="template" xsi:type="string">ui/grid/filters/elements/ui-select</param>
                            <param name="component" xsi:type="string">Magento_Ui/js/form/element/ui-select</param>
                        </select>
                    </filters>
                </templates>
            </settings>-->
        </filters>
        <!--filter setting-->
        <!--massaction-->
        <massaction name="listing_massaction" component="Magento_Ui/js/grid/tree-massactions">
            <!--delete action-->
            <action name="delete">
                <settings>
                    <confirm>
                        <message translate="true">Are you sure you want to delete selected items?</message>
                        <title translate="true">Delete items</title>
                    </confirm>
                    <url path="sumup1/post/massdelete"/>
                    <type>delete</type>
                    <label translate="true">Delete</label>
                </settings>
            </action>
            <!--delete action-->
            <!--change status action-->
            <action name="0">
                <settings>
                    <url path="sumup1/post/poststatus">
                        <param name="status">1</param>
                    </url>
                    <type>enable</type>
                    <label translate="true">Enable</label>
                </settings>
            </action>
            <action name="1">
                <settings>
                    <url path="sumup1/post/poststatus">
                        <param name="status">0</param>
                    </url>
                    <type>disable</type>
                    <label translate="true">Disable</label>
                </settings>
            </action>
        </massaction>
        <!--massaction-->
        <!--pagginng-->
        <paging name="listing_paging">
            <settings>
                <options>
                    <option name="5" xsi:type="array">
                        <item name="value" xsi:type="number">5</item>
                        <item name="label" xsi:type="string">5</item>
                    </option>
                    <option name="10" xsi:type="array">
                        <item name="value" xsi:type="number">10</item>
                        <item name="label" xsi:type="string">10</item>
                    </option>
                    <option name="20" xsi:type="array">
                        <item name="value" xsi:type="number">20</item>
                        <item name="label" xsi:type="string">20</item>
                    </option>
                </options>
                <pageSize>5</pageSize>
            </settings>
        </paging>
        <!--pagginng-->
        <!--export csv-->
        <exportButton class="Magento\Ui\Component\ExportButton">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="options" xsi:type="array">
                    <item name="csv" xsi:type="array">
                        <item name="value" xsi:type="string">csv</item>
                        <item name="label" xsi:type="string" translate="true">CSV</item>
                        <item name="url" xsi:type="string">sumup1/export/PostCsv</item>
                    </item>
                </item>
            </item>
        </argument>
        </exportButton>
        <!--export csv-->
    </listingToolbar>
    <!--toolbar setting-->
    <!--datasource-->
    <dataSource name="nameOfDataSource">
        <argument name="dataProvider" xsi:type="configurableObject">
            <argument name="class" xsi:type="string">TieuMinh\SumUp1\Ui\Component\DataProvider</argument>
            <argument name="name" xsi:type="string">tieuminh_sumup1_post_listing_data_source</argument>
            <argument name="primaryFieldName" xsi:type="string">post_id</argument>
            <argument name="requestFieldName" xsi:type="string">id</argument>
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/provider</item>
                    <item name="update_url" xsi:type="url" path="mui/index/render"/>
                    <item name="storageConfig" xsi:type="array">
                        <item name="indexField" xsi:type="string">post_id</item>
                    </item>
                </item>
            </argument>
        </argument>
    </dataSource>
    <!--datasource-->
    <!--list column-->
    <columns name="spinner_columns">
        <!--select column-->
        <selectionsColumn name="ids">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="resizeEnabled" xsi:type="boolean">true</item>
                    <item name="resizeDefaultWidth" xsi:type="string">55</item>
                    <item name="indexField" xsi:type="string">post_id</item>
                </item>
            </argument>
        </selectionsColumn>
        <!--select column-->
        <!--post id column-->
        <column name="post_id">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sorting" xsi:type="string">asc</item>
                </item>
            </argument>
            <settings>
                <label translate="true">id</label>
                <visible>true</visible>
            </settings>
        </column>
        <!--post id column-->
        <!--post thumbnail column-->
        <column name="thumbnail" class="TieuMinh\SumUp1\Ui\Component\Listing\Columns\Thumbnail" component="Magento_Ui/js/grid/columns/thumbnail" sortOrder="20">
            <settings>
                <altField>post thumbnail</altField>
                <hasPreview>1</hasPreview>
                <addField>true</addField>
                <label translate="true">Thumbnail</label>
                <sortable>false</sortable>
            </settings>
        </column>
        <!--post thumbnail column-->
        <!--post title column-->
        <column name="title">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="editor" xsi:type="array">
                    </item>
                    <item name="label" xsi:type="string" translate="true">Title</item>
                </item>
            </argument>
        </column>
        <!--post title column-->
        <!--post short description column-->
        <column name="short_description">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="label" xsi:type="string" translate="true">short_description</item>
                </item>
            </argument>
        </column>
        <!--post short description column-->
        <!--post content column-->
        <column name="content">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="editor" xsi:type="array">
                        <item name="editorType" xsi:type="string">text</item>
                    </item>
                    <item name="label" xsi:type="string" translate="true">content</item>
                </item>
            </argument>
        </column>
        <!--post content column-->
        <!--post status column-->
        <column name="status">
            <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">TieuMinh\SumUp1\Ui\Component\Listing\Columns\Status</item>
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="editor" xsi:type="array">
                        <item name="editorType" xsi:type="string">text</item>
                    </item>
                    <item name="label" xsi:type="string" translate="true">status</item>
                </item>
            </argument>
        </column>
        <!--post status column-->
        <!--post url key column-->
        <column name="url_key">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="editor" xsi:type="array">
                        <item name="editorType" xsi:type="string">text</item>
                        <item name="validation" xsi:type="array">
                            <item name="required-entry" xsi:type="boolean">true</item>
                        </item>
                    </item>
                    <item name="label" xsi:type="string" translate="true">url_key</item>
                </item>
            </argument>
        </column>
        <!--post url key column-->
        <!--post tag column-->
        <column name="tag_id">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="editor" xsi:type="array">
                        <item name="editorType" xsi:type="string">text</item>
                    </item>
                    <item name="label" xsi:type="string" translate="true">Tag</item>
                </item>
            </argument>
        </column>
        <!--post tag column-->
        <!--post category column-->
        <column name="category_set_id">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="editor" xsi:type="array">
                        <item name="editorType" xsi:type="string">text</item>
                    </item>
                    <item name="label" xsi:type="string" translate="true">Category</item>
                </item>
            </argument>
        </column>
        <!--post category column-->
        <!--pushlish date from column-->
        <column name="publish_date_from">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">dateRange</item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/columns/date</item>
                    <item name="dataType" xsi:type="string">date</item>
                    <item name="label" xsi:type="string" translate="true">Publish Date From</item>
                </item>
            </argument>
        </column>
        <!--pushlish date from column-->
        <!--pushlish date to column-->
        <column name="publish_date_to">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">dateRange</item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/columns/date</item>
                    <item name="dataType" xsi:type="string">date</item>
                    <item name="label" xsi:type="string" translate="true">Publish Date To</item>
                </item>
            </argument>
        </column>
        <!--pushlish date to column-->
        <!--action column-->
        <actionsColumn name="actions" class="TieuMinh\SumUp1\Ui\Component\Listing\Page\PageActions">
            <settings>
                <indexField>post_id</indexField>
                <resizeEnabled>false</resizeEnabled>
                <resizeDefaultWidth>107</resizeDefaultWidth>
            </settings>
        </actionsColumn>
        <!--action column-->
    </columns>
    <!-- list column-->
</listing>
